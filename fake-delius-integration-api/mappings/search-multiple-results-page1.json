{
  "request": {
    "method": "POST",
    "url": "/case-summary/search?page=1&size=20",
    "bodyPatterns": [
      {
        "matchesJsonPath": "[?($.surname =~ /o'bloggs/i)]"
      },
      {
        "matchesJsonPath": "[?($.forename =~ /joe/i)]"
      }
    ]
  },
  "response": {
    "status": 200,
    "bodyFileName": "search-multiple-results-page1.json",
    "headers": {
      "Content-Type": "application/json"
    }
  }
}

{
  "request": {
    "method": "POST",
    "url": "/case-summary/search?page=0&size=20",
    "bodyPatterns": [
      {
        "matchesJsonPath": "[?($.surname =~ /o'connor/i)]"
      },
      {
        "matchesJsonPath": "[?($.forename =~ /john/i)]"
      }
    ]
  },
  "response": {
    "status": 200,
    "bodyFileName": "search-multiple-results-page0.json",
    "headers": {
      "Content-Type": "application/json"
    }
  }
}

{
  "decisionDateTime": "",
  "isExtendedSentence": false,
  "isInCustody": false,
  "mappaLevel": "",
  "policeForce": "",
  "probationArea": "",
  "receivedDateTime": "",
  "riskOfContrabandDetails": ""
}

